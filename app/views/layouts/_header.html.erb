<header class="navbar navbar-inverse">
	<div class="container">
		<%= link_to  "myArtists", root_url, class: "navbar-brand" %>
		<nav>
			<ul class="nav navbar-nav navbar-right">
				<li><%= link_to  "Help", help_path %></li>
				<li><%= link_to  "Home", root_url %></li>
				<% if logged_in? %>
					<li><%= link_to  "Fetch Artists", user_artist_index_path %></li>
					<li><%= link_to  "Profile Page", spotify_data_show_path(current_user) %></li>
					<li><%= link_to	 "Logout", logout_path, method: "delete" %></li>
				<% else %>
					<li> <a href="/auth/spotify"><%= fa_icon('spotify') %> Login </a> </li>
				<% end %>
			</ul>
		</nav>
	</div>
</header>